#: ../netcfg-common.templates:25001
msgid "USB net"
msgstr "USB sarea"

#. Type: text
#. Description
#. :sl2:
#: ../netcfg-common.templates:27001
msgid "Serial-line IP"
msgstr "Serieko lineako IP"

#. Type: text
#. Description
#. :sl2:
#: ../netcfg-common.templates:28001
msgid "Parallel-port IP"
msgstr "Ataka paraleloko IPa"

#. Type: text
#. Description
